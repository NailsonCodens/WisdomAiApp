{
  "expo": {
    "name": "SabedorIA",
    "slug": "wisdomai-app",
    "version": "2.2.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6b39ce"
    },
    "assetBundlePatterns": [
      "**/*"
    ],    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "wisdomai-app",
      "buildNumber": "29"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "wisdomai.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "52576274-a05b-40cb-8381-f093eed0073e"
      }
    },    
    "owner": "nailson.ivs",
    "plugins": [
      "expo-asset",
      "expo-font",
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ]
  }
}
